<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>

	<!--Timbre: Form-->
	<record model="ir.ui.view" id="dz_config_timbre_form">
          <field name="name">dz_config.timbre.form</field>
          <field name="model">config.timbre</field>
	  <field name="arch" type="xml">
	    <form string="Configuration Timbre">
	    <group name="DZ Fiscale" string="Configuration Timbre">
             <field name="name"/>
             <field name="tranche"/>
             <field name="prix"/>
	    </group>
	    <group name="DZ account" string="Configuration Comptes Contreparties">
	      <field name="sale_timbre"/>
              <field name="purchase_timbre"/>
	    </group>
	    <group string="Tranche Minimum du TTC">
               <field name="min_value_go"/>
               <field name="min_value_stop"/>
               <field name="min_value"/>
	    </group>
	    <group string="Maximum">
               <field name="max_value"/>
	    </group>


	  </form>
         </field>
       </record>

	<!--Timbre: Tree-->
	<record model="ir.ui.view" id="dz_config_timbre_tree">
          <field name="name">dz_config.timbre.tree</field>
          <field name="model">config.timbre</field>
          <field name="arch" type="xml">
            <tree string="Configuration Timbre">
               <field name="name"/>
              <field name="tranche"/>
                <field name="prix"/>
               <field name="min_value_go"/>
               <field name="min_value_stop"/>
               <field name="min_value"/>
               <field name="max_value"/>
	    </tree>

         </field>
        </record>

	<record id="action_timbre" model="ir.actions.act_window">
            <field name="name">Configuration Timbre</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">config.timbre</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to create a calcul of timbre.
              </p><p>

              </p>
            </field>
        </record>

        <menuitem action="action_timbre" id="menu_timbre" parent="base.menu_base_config" sequence="90" groups="account.group_account_user"/>

  </data>
</openerp>
