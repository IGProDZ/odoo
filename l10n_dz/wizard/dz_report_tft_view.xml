<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="tft_report_view" model="ir.ui.view">
            <field name="name">TFT Report</field>
            <field name="model">account.tft.report</field>
            <field name="arch" type="xml">
                <form string="TFT Report" version="7.0">
                   <group>
                        <field name="fiscalyear_id"/>
                   </group>
                   <footer>
                        <button name="print_tft_report" string="Print" type="object" class="oe_highlight"  />
						<button icon="gtk-execute" name="xls_export" string="Export" type="object" class="oe_highlight" context="{'xls_export':1}"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                   </footer>
                 </form>
            </field>
        </record>

        <record id="action_account_tft_report" model="ir.actions.act_window">
            <field name="name">Rapport Tableau de Flux de Tr√©sorerie</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">account.tft.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="tft_report_view"/>
            <field name="target">new</field>
        </record>

        <record model="ir.values" id="action_account_tft_report_values">
            <field name="model_id" ref="model_account_tft_report" />
            <field name="name">TFT Report</field>
            <field name="key2">client_print_multi</field>
            <field name="value" eval="'ir.actions.act_window,' +str(ref('action_account_tft_report'))" />
            <field name="key">action</field>
            <field name="model">account.move.line</field>
        </record>

